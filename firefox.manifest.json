{
    "name": "UK Only Ebay",
    "version": "0.1",
    "manifest_version": 2,
    "description": "Sets ebay to UK Only if it isn't already!",
    "content_scripts" : [
        {
        "matches" : ["https://www.ebay.co.uk/sch/*"],
        "js" : ["content_script.js"]
        }
    ],
    "browser_specific_settings": {
        "gecko": {
        "id": "random_email@random.co.uk",
        "strict_min_version": "42.0"
        }
    }
}
